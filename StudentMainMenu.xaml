<Window x:Class="StudentFlight.StudentMainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentFlight"
        mc:Ignorable="d"
        Title="StudentMainMenu" Height="450" Width="800">
    <Grid>
        <Button x:Name="requestFlight" Content="Request flights" HorizontalAlignment="Center" Margin="0,158,0,0" VerticalAlignment="Top" Height="33" Width="270" FontSize="14" FontWeight="Normal" Background="#FF526F9C" Click="requestFlight_Click"/>
        <Button x:Name="logout" Content="Logout" HorizontalAlignment="Center" Margin="0,236,0,0" VerticalAlignment="Top" Height="33" Width="270" FontSize="14" FontWeight="Normal" Background="#FFBB4141" Click="logout_Click"/>

    </Grid>
</Window>
